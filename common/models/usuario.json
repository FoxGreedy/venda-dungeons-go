      "type": "date"
    },
    "dataCriacao": {
      "type": "date"
    },
    "celular": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "nomeCompleto": {
      "type": "string"
    },
    "senha": {
      "type": "string"
    }
  },
  "validations": [],
